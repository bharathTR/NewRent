@model SampleProject.Models.TicketModel
@{
    ViewBag.Title = "TicketView";
    Layout = "~/Views/Shared/MasterMenu.cshtml";
}



@using (Html.BeginForm())
{
    <section class="content-header">
        <h1>
            @ViewBag.Title

        </h1>

    </section>


    <br />

    <section class="box">
        <section class="box-body">
           
            <table id="TicketViewTable" class="display nowrap" cellspacing="0">
                <thead>
                    <tr>
                        <th>Action</th>
                        <th>Ticket Number</th>
                        <th>Type</th>
                        <th>Description</th>
                        <th>Raised Date</th>
                        <th>Status</th>

                    </tr>
                </thead>

            </table>
        </section>
    </section>
}


